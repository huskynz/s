		"@tailwindcss/nesting": "^0.0.0-insiders.565cd3e",
		"autoprefixer": "^10.4.7",
		"nuxt": "3.0.0-rc.4",
		"postcss": "^8.4.14",
		"tailwindcss": "^3.1.4"
	}
